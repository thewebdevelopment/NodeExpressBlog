// Navigation
nav.navbar.navbar-expand-lg.navbar-dark.bg-dark.fixed-top
  .container
    a.navbar-brand(href='#') Webublogoverflow
    button.navbar-toggler(type='button' data-toggle='collapse' data-target='#navbarResponsive' aria-controls='navbarResponsive' aria-expanded='false' aria-label='Toggle navigation')
      span.navbar-toggler-icon
    #navbarResponsive.collapse.navbar-collapse
      ul.navbar-nav.ml-auto
        li.nav-item.active
          a.nav-link(href='/')
            | Home
            span.sr-only (current)
        li.nav-item
          a.nav-link(href='/about') About
        li.nav-item
          a.nav-link(href='/contact') Contact
      .ml-auto.my-2.my-lg-0
        #b-section-navbar-search-form.section(name='Navbar: search form')
          #BlogSearch1.widget.BlogSearch(data-version='2')
            form.form-inline(action='')
              input.form-control.mr-sm-2(aria-label='Search this blog' name='q' placeholder='Search this blog' type='text')
              button.btn.btn-success.my-2.my-sm-0(type='submit') Search